<!DOCTYPE html>
<html>
  <%= render "layouts/head" %>
  <body data-turbolinks="false">
    <%= render "layouts/header" %>
    <div class="container">
      <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
      <% end %>
      <%= yield %>
    </div>
  </body>
</html>
